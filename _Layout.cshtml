<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/css/style.css" />
    <link rel="stylesheet" href="~/css/stylesheet.css" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <link rel="stylesheet" href="https://unpkg.com/swiper@7/swiper-bundle.min.css" />
</head>
<body>
    <header class="header">

        <a asp-page="/q1/Index" class="logo"> <i class="fas fa-spa"></i> Klinik BRHC </a>

        <nav class="navbar">
            <div id="close-navbar" class="fas fa-times"></div>
            <a asp-area="" asp-page="/q1/Index">Home</a>
            <a asp-area="" asp-page="/q1/profile">Profile</a>
            <a asp-area="" asp-page="/q1/company">Company</a>
            <a asp-area="" asp-page="/q1/service">Service</a>
            <a asp-area="" asp-page="/q1/collaboration">Collaboration</a>
        </nav>

        
    </header>
    
            @RenderBody()
    <section class="footer">

        <div class="box-container">

            <div class="box">
                <h3> <i class="fas fa-spa"></i> Klinic BRHC </h3>
                <div class="share">
                    <a href="#" class="fab fa-facebook-f"></a>
                    <a href="#" class="fab fa-twitter"></a>
                    <a href="#" class="fab fa-instagram"></a>
                    <a href="#" class="fab fa-linkedin"></a>
                </div>
            </div>

            <div class="box">
                <h3>quick links</h3>
                <a asp-area="" asp-page="/q1/Index" class="link">Home</a>
                <a asp-area="" asp-page="/q1/profile" class="link">Profile</a>
                <a asp-area="" asp-page="/q1/company" class="link">Company</a>
                <a asp-area="" asp-page="/q1/service" class="link">Service</a>
                <a asp-area="" asp-page="/q1/collaboration" class="link">Collaboration</a>
            </div>

            <div class="box">
                <h3>useful links</h3>
                <a href="#" class="link">help center</a>
                <a href="#" class="link">ask questions</a>
                <a href="#" class="link">send feedback</a>
                <a href="#" class="link">private policy</a>
                <a href="#" class="link">terms of use</a>
            </div>

            

        </div>

        <div class="credit">&copy; <span>Copyright Klinik BRHC -</span> 2022</div>

    </section>

    
    
    <script type="text/javascript" src="~/js/script.js"></script>
    <script type="text/javascript" src="https://unpkg.com/swiper@7/swiper-bundle.min.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
